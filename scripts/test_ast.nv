-- Test all AST node types

func add(a, b)
    ret a + b
end

func factorial(n)
    if n <= 1 then
        ret 1
    else
        ret n * factorial(n - 1)
    end
end

x := 10
y := 20
z := add(x, y)

println(z)

for i := 1, 5 do
    println(i)
end

while x > 0 do
    x := x - 1
end

if x == 0 and y > 10 or z < 100 then
    println("condition met")
end
